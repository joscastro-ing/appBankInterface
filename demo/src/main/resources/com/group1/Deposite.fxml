<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="356.0" prefWidth="286.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group1.controller.DepositeController">
   <children>
      <Button fx:id="doneDeposit" layoutX="30.0" layoutY="231.0" mnemonicParsing="false" onAction="#depositDone" prefHeight="43.0" prefWidth="113.0" text="Done">
         <font>
            <Font name="SansSerif" size="14.0" />
         </font>
      </Button>
      <Label layoutX="50.0" layoutY="35.0" text="MAKE A DEPOSIT">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="depositAmount" layoutX="63.0" layoutY="153.0" prefHeight="24.0" prefWidth="172.0" />
      <Label layoutX="112.0" layoutY="117.0" text="Amount">
         <font>
            <Font name="SansSerif" size="18.0" />
         </font>
      </Label>
      <Button fx:id="cancel" layoutX="149.0" layoutY="231.0" mnemonicParsing="false" onAction="#cancelDeposit" prefHeight="43.0" prefWidth="113.0" text="Cancel">
         <font>
            <Font name="SansSerif" size="14.0" />
         </font>
      </Button>
      <Hyperlink layoutX="30.0" layoutY="305.0" onMouseClicked="#cancelDeposit" text="Return to Account" />
   </children>
</AnchorPane>
