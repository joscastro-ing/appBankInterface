<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="331.0" prefWidth="413.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group1.controller.CreateNewAccountController">
   <children>
      <Label layoutX="51.0" layoutY="33.0" text="CREATE A NEW ACCOUNT">
         <font>
            <Font name="SansSerif Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="86.0" text="Initial Amount">
         <font>
            <Font name="SansSerif" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="initialAmount" layoutX="51.0" layoutY="120.0" prefHeight="25.0" prefWidth="222.0" />
      <ComboBox fx:id="typeOfAccount" layoutX="51.0" layoutY="166.0" onShowing="#showList" prefWidth="150.0" promptText="Type of Account" />
      <Button fx:id="create" layoutX="66.0" layoutY="237.0" mnemonicParsing="false" onAction="#createNew" prefHeight="38.0" prefWidth="112.0" text="Create" />
      <Button fx:id="cancelCreate" layoutX="232.0" layoutY="237.0" mnemonicParsing="false" onAction="#cancelCreateAccount" prefHeight="38.0" prefWidth="112.0" text="Cancel" />
      <Hyperlink layoutX="22.0" layoutY="286.0" onMouseClicked="#cancelCreateAccount" text="Return to Account" />
   </children>
</AnchorPane>
